#include<iostream>
#include<cstdio>
#include<cstring>

#define maxx 120005

using namespace std;

int main()
{
	int price[6]={1,2,3,4,5,6};
	int number[6],dp[maxx];
	int test=0;
	while(1){
		int sum=0,check=0;
		for(int i=0;i<6;i++){
			scanf("%d",&number[i]);
			if(number[i])	check=1;
		}
		if(check==0)	break;
		for(int i=0;i<6;i++){
			sum += number[i]*price[i];
		}
		memset(dp,0,sizeof(dp));
		dp[0]=1;
		int m=sum%2;
		for(int i=0;i<6;i++)
			for(int k=0;k<=price[i];k++){
				int left=number[i];
				for(int j=0;j<=m;j+=price[i])
					if(dp[j])	left=number[i];
					else if(left>0){
						left--;
						dp[j]=true;
					}
			}
		//for(int i=0;i<sum;i++)
		//	printf("%2d",dp[i]);
		//printf("\n");
		printf("Collection #%d\n",test++);
		if(sum%2==0 &&dp[sum/2]==1)	printf("Can be divided.\n\n");
		else				printf("can't be divided.\n\n");
	}
	return 0;
}
