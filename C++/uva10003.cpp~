#include<iostream>
#include<cstdio>
#include<cstring>

#define maxx 55
#define INF 2e9
#define maxlen 1005

using namespace std;

int dp[maxlen][maxlen];
int cut[maxx],len,n;

int cutting(int a,int b)
{
	if(dp[a][b]!= -1)	return dp[a][b];
	int tmp;
	for(int i=a+1;i<b;i++){
		tmp=cutting(a,i)+cutting(i,b)+cut[b]-cut[a];
		if(dp[a][b]<0 || tmp<dp[a][b])	dp[a][b]=tmp;
	}
	return dp[a][b];
}
int main()
{
	while(scanf("%d",&len)!=EOF && len){
		scanf("%d",&n);
		memset(dp,-1,sizeof(dp));
		cut[0]=0;
		for(int i=1;i<=n;i++)
			scanf("%d",&cut[i]);
		cut[n+1]=len;
		for(int i=0;i<=n;i++)
			dp[i][i+1]=0;
		printf("The minimum cutting is %d.\n",cutting(0,n+1));
	}
	return 0;
}
