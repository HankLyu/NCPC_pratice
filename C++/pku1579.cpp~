#include<iostream>
#include<cstdio>

using namespace std;

int dp[30][30][30]={0};

int w(int a,int b,int c)
{
	if(dp[a][b][c]!=0)
		return dp[a][b][c];
	else if(a<1||b<1||c<1)
		return 1;
	else if(a<b && b<c)
		dp[a][b][c]=w(a,b,c-1)+w(a,b-1,c-1)-w(a,b-1,c);
	else
		dp[a][b][c]=w(a-1,b,c)+w(a-1,b-1,c)+w(a-1,b,c-1)-w(a-1,b-1,c-1);
	return dp[a][b][c];
}
int main()
{
	int a,b,c,ans;
	while(scanf("%d %d %d",&a,&b,&c)!=EOF){
		if(a==-1&&b==-1&&c==-1)	break;
		if(a<1 || b<1 || c<1)
			ans=1;
		else if(a>20 || b>20 || c>20)
			ans=w(20,20,20);
		else	ans=w(a,b,c);
		printf("w(%d, %d, %d) = %d\n",a,b,c,ans);
	}
	return 0;
}
